{
  "manifest_version": 3,
  "name": "ClearTab",
  "version": "1.0",
  "description": "A glassmorphism-inspired custom dashboard.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
  "https://script.google.com/*",
  "https://script.googleusercontent.com/*"
],
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/icon.png"
  }
}